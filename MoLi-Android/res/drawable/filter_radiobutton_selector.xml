<selector xmlns:android="http://schemas.android.com/apk/res/android">

   <!--  <item android:state_checked="true"  android:drawable="@drawable/filter_radiobutton_press"/>
    <item android:drawable="@drawable/filter_radiobutton_nomal"/> -->
    
    <item android:state_checked="true">
        <shape>
            <corners android:radius="6dp" />

            <solid android:color="@color/line_color" />

            <stroke android:width="1px" android:color="@color/line_color" />

            <padding android:bottom="5dp" android:left="12dp" android:right="12dp" android:top="5dp" />
        </shape>
    </item>
    
    <item>
        
        <shape>
            <corners android:radius="6dp" />

            <solid android:color="@color/white" />
            
            <stroke android:width="1px" android:color="@color/line_color" />

            <padding android:bottom="5dp" android:left="12dp" android:right="12dp" android:top="5dp" />
        </shape>
        
    </item>
</selector>
