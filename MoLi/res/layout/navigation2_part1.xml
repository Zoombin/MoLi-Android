<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:scaleType="fitCenter"
        android:src="@drawable/navigation_p2_part1_shou" />

    <ImageView
        android:id="@+id/nav_p1_ji"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dip"
        android:layout_marginTop="30dip"
        android:scaleType="fitXY"
        android:src="@drawable/navigation_p2_part1_ji" />

    <ImageView
        android:id="@+id/nav_p1_you"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_marginRight="20dip"
        android:layout_marginTop="35dip"
        android:scaleType="fitXY"
        android:src="@drawable/navigation_p2_part1_you" />

    <ImageView
        android:id="@+id/nav_p1_hui"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="30dip"
        android:layout_marginTop="95dip"
        android:scaleType="fitXY"
        android:src="@drawable/navigation_p2_part1_hui" />

    <ImageView
        android:id="@+id/nav_p1_li"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="75dip"
        android:scaleType="fitXY"
        android:src="@drawable/navigation_p2_part1_li" />

    <ImageView
        android:id="@+id/nav_p1_gou"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_marginRight="30dip"
        android:layout_marginTop="95dip"
        android:scaleType="fitXY"
        android:src="@drawable/navigation_p2_part1_gou" />

    <ImageView
        android:id="@+id/nav_p1_gui"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="75dip"
        android:layout_marginTop="155dip"
        android:scaleType="fitXY"
        android:src="@drawable/navigation_p2_part1_gui" />

    <ImageView
        android:id="@+id/nav_p1_dai"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_marginRight="75dip"
        android:layout_marginTop="155dip"
        android:scaleType="fitXY"
        android:src="@drawable/navigation_p2_part1_dai" />

    <ImageView
        android:id="@+id/nav_p1_da"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="235dip"
        android:scaleType="fitXY"
        android:src="@drawable/navigation_p2_part1_da" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:scaleType="fitCenter"
        android:src="@drawable/navigation_p2_part1_shou1" />

</RelativeLayout>